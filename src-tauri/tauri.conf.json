{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MachineID-Manage",
  "version": "0.1.0",
  "identifier": "com.machineid.manage",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "MachineID-Manage",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
